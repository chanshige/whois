{
  "name": "chanshige/whois",
  "description": "'chanshige/whois' is that helps your domain registered information(whois) search.",
  "keywords": [
    "whois",
    "domain",
    "nic"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Shigeki Tanaka",
      "email": "dev@shigeki.tokyo"
    }
  ],
  "autoload": {
    "files": [
      "extend.functions.php"
    ],
    "psr-4": {
      "Chanshige\\": "src/",
      "Handler\\": "src/Handler",
      "Exception\\": "src/Exception"
    }
  },
  "autoload-dev": {
    "files": [
      "extend.functions.php"
    ],
    "psr-4": {
      "Chanshige\\": "tests/",
      "Handler\\": "tests/Handler"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "*",
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "test": [
      "phpmd src text ./phpmd.xml",
      "phpcs src",
      "phpunit"
    ]
  }
}
