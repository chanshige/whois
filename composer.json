{
  "name": "chanshige/whois",
  "description": "'chanshige/whois' is that helps your domain registered information(whois) search.",
  "keywords": [
    "whois",
    "domain",
    "nic"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Shigeki Tanaka",
      "email": "dev@shigeki.tokyo"
    }
  ],
  "autoload": {
    "files": [
      "src-files/common.php",
      "src-files/server_list.php",
      "extend.functions.php"
    ],
    "psr-4": {
      "Chanshige\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chanshige\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "*",
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "test": [
      "phpmd src text ./phpmd.xml",
      "phpcs src",
      "phpunit"
    ]
  }
}
