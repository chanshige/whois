{
  "name": "chanshige/whois",
  "description": "'chanshige/whois' is that helps your domain registered information(whois) search.",
  "keywords": [
    "whois",
    "domain",
    "nic",
    "auradi"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Shigeki Tanaka",
      "email": "dev@shigeki.tokyo"
    }
  ],
  "autoload": {
    "files": [
      "src/globals.php"
    ],
    "psr-4": {
      "Chanshige\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chanshige\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "aura/di": "^4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "*",
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "qa": [
      "phpmd src text ./phpmd.xml",
      "phpcs src"
    ],
    "tests": [
      "@qa",
      "@test"
    ]
  }
}
